import java.util.Scanner;

public class ListaCiudades{
	
	public static void main(String[] args) {
		
	List<String> ciudades = new ArrayList<>();
	
	for (int contador =1; contador<=5;contador++) {
		
		Scanner leer = new Scanner(System.in);
        System.out.print("Introduzca ciudad favorita: ");
        num1 = leer.nextLine();
		
	}

	
	/* ciudades.add("Tafí del Valle");
	ciudades.add("Cosquin");
	ciudades.add("Federación");
	ciudades.add("Segunda Usina");
	ciudades.add("Cafayate"); */
	
	System.out.println("Lista de ciudades preferidas de Argentina");
	
	for (Iterator<String> iterator = ciudades.iterator(); iterator.hasNext();) {
		
		String ciudad = (String) iterator.next();
		System.out.println(ciudad);
		
	}
	
}
	
}